{% block body %}

    <section class="ftco-section ">
        <div class="container">
            <div class="row justify-content-center mb-5 pb-3">
                <div class="col-md-7 text-center heading-section ftco-animate">
                    <span class="subheading">Agenda</span>
                    <h2 class="mb-4"><span>Nos évènement</span></h2>
                </div>
            </div>
            <div class="ftco-search">
                <div class="row">
                    <div class="col-md-12 nav-link-wrap">
                        <div class="nav nav-pills d-flex text-center" id="v-pills-tab" role="tablist"
                             aria-orientation="vertical">
                            <a class="nav-link ftco-animate active" id="v-pills-1-tab" data-toggle="pill"
                               href="#v-pills-1" role="tab" aria-controls="v-pills-1" aria-selected="true">Semaine 1<span>{{ "now" |date("m/y") }}</span></a>

                            <a class="nav-link ftco-animate" id="v-pills-2-tab" data-toggle="pill" href="#v-pills-2"
                               role="tab" aria-controls="v-pills-2" aria-selected="false">semaine 2 <span>{{ "now"  |date("m/y") }}</span></a>

                            <a class="nav-link ftco-animate" id="v-pills-3-tab" data-toggle="pill" href="#v-pills-3"
                               role="tab" aria-controls="v-pills-3" aria-selected="false">semaine 3 <span>{{ "now"  |date("m/y") }}</span></a>

                            <a class="nav-link ftco-animate" id="v-pills-4-tab" data-toggle="pill" href="#v-pills-4"
                               role="tab" aria-controls="v-pills-4" aria-selected="false">semaine 4 <span>{{ "now"  |date("m/y") }}</span></a>

                        </div>
                    </div>
                    <div class="col-md-12 tab-wrap">

                        <div class="tab-content" id="v-pills-tabContent">



                                <div class="tab-pane fade show active" id="v-pills-1" role="tabpanel" aria-labelledby="day-1-tab">
                                    {% for c in l1 %} <div class="speaker-wrap ftco-animate d-flex">
                                        <div class="img speaker-img"
                                             style="background-image: url(images/person_1.jpg);"></div>
                                        <div class="text pl-md-5">

                                            <span class="time">{{ c.delais}} </span>
                                            <h2><a href="#">{{ c. theme }}</a></h2>
                                            <h2><a href="#">{{ c. titre }}</a></h2>
                                            <p>{{ c.description }}</p>
                                            <h3 class="speaker-name">&mdash; <span class="position" > {{ c.start |date("m/d/Y") }}</span> <span
                                                        class="position">{{ c.end|date("m/d/Y") }}</span>
                                            </h3>

                                            <h3>{{ c.localisation }}</h3>

                                        </div>
                                    </div>   {% endfor %}
                                </div>

                                    <div class="tab-pane fade" id="v-pills-2" role="tabpanel"
                                         aria-labelledby="v-pills-day-2-tab">
                                        {% for c in l2 %}   <div class="speaker-wrap ftco-animate d-flex">
                                            <div class="img speaker-img"
                                                 style="background-image: url(images/person_1.jpg);"></div>
                                            <div class="text pl-md-5">

                                                <span class="time">{{ c.delais }}</span>
                                                <h2><a href="#">{{ c. theme }}</a></h2>
                                                <h2><a href="#">{{ c. titre }}</a></h2>
                                                <p>{{ c.description }}</p>
                                               <h3 class="speaker-name">&mdash; <span class="position">{{ c.start|date("m/d/Y") }}</span>
                                                    <span class="position">{{ c.end|date("m/d/Y")}}</span></h3>
                                                <h3>{{ c.localisation }}</h3>

                                            </div>
                                        </div> {% endfor %}
                                    </div>

                                    <div class="tab-pane fade" id="v-pills-3" role="tabpanel"
                                         aria-labelledby="v-pills-day-3-tab">
                                        {% for c in l3 %} <div class="speaker-wrap ftco-animate d-flex">
                                            <div class="img speaker-img"
                                                 style="background-image: url(images/person_1.jpg);"></div>
                                            <div class="text pl-md-5">

                                                <span class="time">{{ c.delais }}</span>
                                                <h2><a href="#">{{ c. theme }}</a></h2>
                                                <h2><a href="#">{{ c. titre }}</a></h2>
                                                <p>{{ c.description }}</p>
                                                <h3 class="speaker-name">&mdash; <span class="position" >{{ c.start|date("m/d/Y")  }}</span> <span
                                                            class="position">{{ c.end|date("m/d/Y")  }}</span></h3>
                                                <h3>{{ c.localisation }}</h3>

                                            </div>
                                        </div>{% endfor %}
                                    </div>

                                    <div class="tab-pane fade" id="v-pills-4" role="tabpanel"
                                         aria-labelledby="v-pills-day-4-tab">
                                        {% for c in l4 %}
                                        <div class="speaker-wrap ftco-animate d-flex">
                                            <div class="img speaker-img"
                                                 style="background-image: url(images/person_1.jpg);"></div>
                                            <div class="text pl-md-5">

                                                <span class="time">{{ c.delais }}</span>
                                                <h2><a href="#">{{ c. theme }}</a></h2>
                                                <h2><a href="#">{{ c. titre }}</a></h2>
                                                <p>{{ c.description }}</p>
                                                <h3 class="speaker-name">&mdash; <span class="position"> {{ c.start |date("m/d/Y")  }}</span> <span
                                                            class="position">{{ c.end |date("m/d/Y") }}</span></h3>
                                                <h3>{{ c.localisation }}</h3>

                                            </div>
                                        </div> {% endfor %}
                                    </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>





    <div id="footer" style="margin-bottom: 100px">
        <div class="footer-newsletter">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-6">
                        <h4>Inscrivez-vous</h4>
                        <p>Inscrivez-vous pour recevoir les notifications des nouveautés des domaines l'environnement,
                            l'hygiène et la santé sécurité au travail</p>
                        <form action="" method="post">
                            <input type="email" name="email"><input type="submit" value="S'abonner">
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>




{% endblock %}